{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/vDevPreview/MicrosoftTeams.schema.json",
    "manifestVersion": "devPreview",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Frank Chen",
        "websiteUrl": "https://www.example.com",
        "privacyUrl": "https://www.example.com/privacy",
        "termsOfUseUrl": "https://www.example.com/termofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "cde-helper${{APP_NAME_SUFFIX}}",
        "full": "full name for cde-helper"
    },
    "description": {
        "short": "cde-helper allows you to find tasks which you have done with various customers",
        "full": "cde-helper is the ultimate tool for managing your tasks done for M365 CDE support. With its intuitive interface and powerful features, you'll be able to easily find your tasks by name, customer, status, tag and time. \n\n **Why Choose cde-helper:** \n\n cde-helper is the perfect solution for individual to keep track of their taskâ€™s status. \n\n **Features and Benefits:** \n\n - Easy task Search through Microsoft Copilot. Simply start by saying, 'Find tasks done for customer xyz' \r  - User-Friendly Interface: cde-helper's intuitive interface makes it easy to navigate and use \n\n **Availability:** \n\n To use cde-helper, you'll need an active Microsoft 365 account . Ensure that your administrator enables the app for your Microsoft 365 account."
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupChat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [
        {
            "botId": "${{BOT_ID}}",
            "commands": [
                {
                    "id": "searchQuery",
                    "initialRun": true,
                    "context": [
                        "compose",
                        "commandBox"
                    ],
                    "description": "Search tasks by name, customer, status and tag",
                    "title": "Task Search",
                    "type": "query",
                    "parameters": [
                        {
                            "name": "taskName",
                            "title": "Task name",
                            "description": "Enter a task name her",
                            "inputType": "text"
                        },
                        {
                            "name": "customerName",
                            "title": "Customer name",
                            "description": "Enter the customer name of the task",
                            "inputType": "text"
                        },
                        {
                            "name": "taskStatus",
                            "title": "Task Status",
                            "description": "Enter the status of the task. convert status to one of 'to do', 'doing' or 'done'. leave it empty if cannot convert",
                            "inputType": "text"
                        },
                        {
                            "name": "taskCreationDate",
                            "title": "Creation Date",
                            "description": "Task creation date range.convert date format based on today like yyyy/MM/dd.Output format:{\"s\":\"<start date>\", \"e\":\"<end date>\"}",
                            "inputType": "text"
                        }
                    ]
                },
                {
                    "id": "addMessage",
                    "description": "Add current message to cde-helper",
                    "title": "Add to cde-helper",
                    "type": "action",
                    "fetchTask": true,
                    "context": [
                        "message"
                    ]
                }
            ]
        }
    ],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "https://app.vssps.visualstudio.com",
        "https://login.microsoftonline.com",
        "https://msft.sts.microsoft.com",
        "https://certauth.msft.sts.microsoft.com",
        "dev.azure.com",
        "${{BOT_DOMAIN}}"
    ],
    "showLoadingIndicator": false,
    "isFullScreen": false,
    "defaultBlockUntilAdminAction": false,
    "authorization": {
        "permissions": {
            "resourceSpecific": [
