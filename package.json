{
    "name": "podhelper",
    "version": "1.0.0",
    "description": "Microsoft Teams Toolkit message extension search sample",
    "engines": {
        "node": "16 || 18 || 20"
    },
    "author": "Microsoft",
    "license": "MIT",
    "main": "./lib/src/index.js",
    "scripts": {
        "dev:teamsfx-old": "env-cmd --silent -f .localConfigs npm run dev",
        "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run watch",
        "dev:teamsfx2": "env-cmd --silent -f .localConfigs npm run start",
        "dev:teamsfx:testtool": "env-cmd --silent -f .localConfigs.testTool npm run dev",
        "dev:teamsfx:launch-testtool": "env-cmd --silent -f env/.env.testtool teamsapptester start",
        "dev": "nodemon --exec node --inspect=9239 --signal SIGINT -r ts-node/register ./src/index.ts",
        "build-old": "tsc --build",
        "build-old1": "tsc --build && copyfiles ./public/*.html lib/",
        "start": "node ./dist/index.js",
        "test-old": "echo \"Error: no test specified\" && exit 1",
        "watch-old": "nodemon --exec \"npm run start\"",
        "watch": "webpack --watch --mode development",
        "build:dev": "webpack --mode development",
        "build:prod": "webpack --mode production",
        "outlook:startlocal": "office-addin-debugging start appPackage/build/local/manifest.json",
        "outlook:stoplocal": "office-addin-debugging stop appPackage/build/local/manifest.json",
        "outlook:startdev": "office-addin-debugging start appPackage/build/dev/manifest.json",
        "outlook:stopdev": "office-addin-debugging stop appPackage/build/dev/manifest.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "@azure/cosmos": "^4.0.0",
        "@fluentui/react": "^8.115.6",
        "adaptive-expressions": "^4.20.0",
        "adaptivecards": "^3.0.1",
        "adaptivecards-templating": "^2.3.1",
        "axios": "^1.6.5",
        "botbuilder": "^4.20.0",
        "jwt-decode": "^4.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "node-html-markdown": "^1.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.50.1",
        "react-quill": "^2.0.0",
        "react-router-dom": "^5.2.0",
        "restify": "^11.1.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/node": "^16.0.0",
        "@types/office-js": "^1.0.376",
        "@types/office-runtime": "^1.0.35",
        "@types/restify": "^8.5.5",
        "copy-webpack-plugin": "^12.0.2",
        "copyfiles": "^2.4.1",
        "css-loader": "^6.5.1",
        "env-cmd": "^10.1.0",
        "html-webpack-plugin": "^5.5.0",
        "ngrok": "^5.0.0-beta.2",
        "nodemon": "^3.0.1",
        "nodemon-webpack-plugin": "^4.8.2",
        "office-addin-debugging": "^5.0.16",
        "shx": "^0.3.3",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.4.0",
        "typescript": "^5.2.2",
        "webpack": "^5.76.3",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "4.13.1",
        "webpack-node-externals": "^3.0.0"
    }
}